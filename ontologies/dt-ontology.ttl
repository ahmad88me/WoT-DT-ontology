                               rdfs:domain :DigitalTwinService ;
                               rdfs:range <https://www.w3.org/2019/wot/td#InteractionAffordance> ;
                               rdfs:label "contains interaction affordance" .


###  https://w3id.org/def/digitaltwin#hasComponent
:hasComponent rdf:type owl:ObjectProperty ;
              rdfs:comment "Property that describes the different components that the dt:PhysicalEntitty contains."@en ;
              rdfs:label "has component" .


###  https://w3id.org/def/digitaltwin#hasConnection
:hasConnection rdf:type owl:ObjectProperty ;
               rdfs:domain :DigitalTwinConnection ;
               rdfs:range :Connection ;
               rdfs:comment "Property that describes the different connections that the dt:DigitalTwinConnections contains."@en ;
               rdfs:label "has connection" .


###  https://w3id.org/def/digitaltwin#hasConsumer
:hasConsumer rdf:type owl:ObjectProperty ;
             rdfs:domain :Connection ;
             rdfs:range :ConnectionPoint ;
             rdfs:label "has consumer" .


